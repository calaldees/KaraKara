{
  "name": "browser3",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "serve": "vite --host 127.0.0.1 --port 1236",
    "build": "tsc -b && vite build --base /browser3/",
    "cypress": "npx cypress open",
    "format": "prettier --write '{src,cypress}/**/*.ts{,x}' --tab-width 4 --trailing-comma all",
    "preview": "npm run build && vite preview --host 127.0.0.1 --port 1236 --base /browser3/",
    "lint": "eslint src",
    "test": "vitest",
    "test-once": "vitest run"
  },
  "dependencies": {
    "@shish2k/react-mqtt": "^0.0.3",
    "@shish2k/react-use-servertime": "^0.0.4",
    "@ungap/global-this": "^0.4.4",
    "abortcontroller-polyfill": "^1.7.8",
    "qrcode.react": "^4.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.3.0",
    "u8-mqtt": "^0.6.6",
    "usehooks-ts": "^3.1.1",
    "vite-plugin-svgr": "^4.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-pwa": "^0.21.1"
  }
}
