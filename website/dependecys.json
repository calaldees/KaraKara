{
    "jquery": {
        "source": "http://code.jquery.com/jquery-VERSION.min.js",
        "version": "2.1.4",
        "target": "static/jquery.min.js"
    },

    "jquery_map": {
        "source": "http://code.jquery.com/jquery-VERSION.min.map",
        "version": "2.1.4",
        "target": "static/jquery-VERSION.min.map"
    },
    
    "jquery_cookie": {
        "source": "http://raw.github.com/carhartl/jquery-cookie/vVERSION/jquery.cookie.js",
        "version": "1.4.0",
        "target": "static/jquery.cookie.js"
    },

    "jquery_sortable": {
        "source": "https://raw.github.com/farhadi/html5sortable/master/jquery.sortable.js",
        "version": "0.0.0",
        "target": "static/jquery.sortable.js"
    },

    "jquery_mobile": {
        "source": "http://code.jquery.com/mobile/VERSION/jquery.mobile-VERSION.min.js",
        "version": "1.4.5",
        "target": "static/jquery.mobile.min.js"
    },
    "jquery_mobile_css": {
        "source": "http://code.jquery.com/mobile/VERSION/jquery.mobile-VERSION.min.css",
        "version": "1.4.5",
        "target": "static/jquery.mobile.min.css"
    },
    "jquery_mobile_map": {
        "source": "http://code.jquery.com/mobile/VERSION/jquery.mobile-VERSION.min.map",
        "version": "1.4.5",
        "target": "static/jquery.mobile-VERSION.min.map"
    },
    "jquery_mobile_spinner": {
        "source": "http://code.jquery.com/mobile/VERSION/images/ajax-loader.gif",
        "version": "1.4.5",
        "target": "static/images/ajax-loader.gif"
    },
    
    "yui-reset": {
