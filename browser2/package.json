{
    "name": "browser2",
    "dependencies": {
        "@shish2k/hyperapp-hash-state": "^2.0.11",
        "@shish2k/hyperapp-localstorage": "^1.0.2",
        "@shish2k/hyperapp-mqtt": "^1.1.7",
        "@shish2k/hyperapp-synced-interval": "^0.0.4",
        "hyperapp": "^2.0.22",
        "hyperapp-fx": "^2.0.0-beta.2",
        "hyperapp-jsx-pragma": "^1.3.0",
        "qrcode-generator": "^1.4.4"
    },
    "scripts": {
        "build": "parcel build --public-url . src/index.html",
        "watch": "parcel watch --public-url . src/index.html",
        "serve": "parcel serve --port 1234 src/index.html",
        "format": "prettier --write 'src/**/*.tsx' --tab-width 4 --trailing-comma all",
        "test": "jest --verbose"
    },
    "repository": "https://github.com/calaldees/KaraKara/",
    "license": "GPL-3.0-only",
    "devDependencies": {
        "@parcel/optimizer-data-url": "^2.8.0",
        "@parcel/packager-raw-url": "^2.8.0",
        "@parcel/packager-xml": "^2.8.0",
        "@parcel/transformer-inline-string": "^2.8.0",
        "@parcel/transformer-sass": "^2.8.0",
        "@types/jest": "^28.1.1",
        "events": "^3.3.0",
        "jest": "^28.1.1",
        "parcel": "^2.8.0",
        "parcel-config-pwa-manifest": "^0.1.0",
        "prettier": "^2.4.1",
        "process": "^0.11.10",
        "punycode": "^1.4.1",
        "querystring-es3": "^0.2.1",
        "ts-jest": "^28.0.4",
        "url": "^0.11.0"
    },
    "pwaManifest": {
        "development": {
            "disabled": true
        },
        "name": "KaraKara",
        "shortName": "KaraKara",
        "startURL": ".",
        "theme": "#e9e9e9",
        "display": "standalone",
        "orientation": "portrait",
        "generateIconOptions": {
            "baseIcon": "./src/static/favicon.svg",
            "sizes": [
                192,
                384,
                512
            ],
            "genFavicons": true
        }
    }
}
