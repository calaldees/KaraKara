{
  "name": "player3",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "serve": "vite --host 127.0.0.1 --port 1237",
    "build": "tsc && vite build --base /player3/",
    "format": "prettier --write 'src/**/*.ts{,x}' --tab-width 4 --trailing-comma all",
